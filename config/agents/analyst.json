{
    "provider": "openrouter",
    "model": "google/gemini-2.0-flash-001",
    "temperature": 0,
    "max_tokens": 300,
    "json_mode": true,
    "max_turns": 12,
    "tool_config": {
        "info_retriever": {
            "type": "info",
            "config_path": "config/tools/info_database/{dataset}.json"
        },
        "interaction_retriever": {
            "type": "interaction",
            "config_path": "config/tools/interaction/{dataset}.json"
        }
    }
}
