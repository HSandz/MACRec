{
    "model_type": "openrouter",
    "model_name": "google/gemini-2.0-flash-lite-001",
    "temperature": 0,
    "max_tokens": 300,
    "json_mode": true,
    "top_k": 10,
    "enable_compression": false,
    "compression_ratio": 0.9,
    "tool_config": {
        "candidate_retriever": {
            "type": "embedding_retriever",
            "config_path": "config/tools/embedding_retriever.json"
        },
        "info_retriever": {
            "type": "info",
            "config_path": "config/tools/info_database/{dataset}.json"
        }
    }
}


